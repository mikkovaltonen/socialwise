{
  "name": "demand-manager-batch-processor",
  "version": "1.0.0",
  "description": "Batch processing for substrate family AI analysis",
  "type": "module",
  "main": "dist/batchProcessor.js",
  "scripts": {
    "build": "tsc",
    "start": "node --loader ts-node/esm batchProcessor.ts",
